@page
@model IndexModel
<h1>Lista Prodotti</h1>

<ul>
    @foreach (var prodotto in Model.Prodotti)
    {
        <li>@prodotto.Id - @prodotto.Nome - €@prodotto.Prezzo</li>
    }
</ul>

<p><strong>Ambiente:</strong> @Model.Ambiente</p>
<p><strong>Percorso JSON:</strong> @Model.filePath</p>
<p><strong>Ultimo Aggiornamento:</strong></p>